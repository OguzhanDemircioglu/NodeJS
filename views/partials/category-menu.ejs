<div class="list-group">
    <% for( let index=0; index < categories.length; index++ ) { %>
        <a href="" class="list-group-item list-group-item-action-active">
            <%=categories[index]%>
        </a>
        <% } %>

            <% categories.forEach(element=> { %>
                <a href="" class="list-group-item list-group-item-action-active">
                    <%=element%>
                </a>
                <% }) %>
</div>